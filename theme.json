{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "border": {
            "radius": false,
            "color": false,
            "style": false,
            "width": false
        },
        "color": {
            "custom": true,
            "customDuotone": false,
            "customGradient": false,
            "duotone": [],
            "gradients": [],
            "link": false,
            "palette": [],
            "text": true,
            "background": true,
            "defaultGradients": false,
            "defaultPalette": true
        },
        "custom": {},
        "layout": {
            "contentSize": "650px",
            "wideSize": "1000px"
        },
        "spacing": {
            "margin": false,
            "padding": false,
            "blockGap": null,
            "units": [
                "%",
                "px",
                "em",
                "rem",
                "vh",
                "vw"
            ]
        },
        "typography": {
            "customFontSize": true,
            "lineHeight": true,
            "dropCap": false,
            "fontStyle": true,
            "fontWeight": true,
            "letterSpacing": false,
            "textDecoration": false,
            "textTransform": false,
            "fontSizes": [
                {
                    "slug": "small",
                    "size": "0.875rem",
                    "name": "Small"
                },
                {
                    "slug": "medium",
                    "size": "1rem",
                    "name": "medium"
                },
                {
                    "slug": "large",
                    "size": "1.25rem",
                    "name": "large"
                },
                {
                    "slug": "larger",
                    "size": "1.5rem",
                    "name": "larger"
                }
            ]
        },
        "blocks": {
            "core/paragraph": {
                "color": {},
                "custom": {},
                "layout": {},
                "spacing": {},
                "typography": {}
            },
            "core/heading": {}
        }
    }
}